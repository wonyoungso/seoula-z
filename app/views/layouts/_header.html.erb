<div id="header_wrapper">
  <div id="header">
    <div class="wrapper">
      <div id="by_category">
        <%= image_tag asset_path('by_category.png'), :id => 'by_category_img' %>  
        <%= link_to '<', '#', :class => 'prev_btn' %>
        <div class="category_list">
          <div class="category">
            <div class="wrapper">
              <%= link_to '에너지', '#', :id => 'category_energy_btn', :class => 'category_btn'  %>
            </div>
            <em>에너지</em>
          </div>
          <div class="category">
            <div class="wrapper">
              <%= link_to '교육', '#', :id => 'category_education_btn', :class => 'category_btn'  %>
            </div>
            <em>교육</em>
          </div>
          <div class="category">
            <div class="wrapper">
              <%= link_to '주거', '#', :id => 'category_residence_btn', :class => 'category_btn'  %>
            </div>
            <em>주거</em>
          </div>
          <div class="category">
            <div class="wrapper">
              <%= link_to '교통', '#', :id => 'category_transportation_btn', :class => 'category_btn'  %>
            </div>
            <em>교통</em>
          </div>
        </div>
        <%= link_to '>', '#', :class => 'next_btn' %>
      </div>
      <div id="logo">
        <h1>
          <%= link_to 'Seoul A-Z Project', root_path %>
        </h1>
      </div>
      <div id="by_latest">
        <%= image_tag asset_path('by_latest.png'), :id => 'by_latest_img' %>
        <div class="latest_list">
          에너지 사용량과 사업체와의 관계 <em>(week 2)</em>
        </div>
      </div>
    </div>
    <div class="CB"></div>
  </div>
  
  <div id="toggle_header">
    <%= link_to '닫기 / 펼치기', '#', :id => 'toggle_header_btn', :class => 'expanded' %>
  </div>
</div>



<script type="text/javascript">
  $(document).ready(function(e){
    CDNM.views.menu_view();
  });
</script>